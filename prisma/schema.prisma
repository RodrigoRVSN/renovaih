generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  username  String
  email     String
  avatar    String
  points    Int
  Ranking   Ranking? @relation(fields: [rankingId], references: [id])
  rankingId Int?
}

model Ranking {
  id      Int    @id @default(autoincrement())
  User    User[]
}
